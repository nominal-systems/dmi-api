{
  "name": "diagnostic-modality-integration-api",
  "version": "1.11.4",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t nominal-systems/dmi-api . --build-arg GHP_TOKEN=$GHP_TOKEN",
    "postdocker:build": "docker tag nominal-systems/dmi-api:latest nominal-systems/dmi-api:$npm_package_version",
    "build": "nest build",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push --follow-tags",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node --transpile-only ./node_modules/typeorm/cli.js -f src/config/ormconfig.ts",
    "migration:run": "npm run typeorm migration:run",
    "migration:create": "npm run typeorm -- migration:create -n",
    "migration:generate": "npm run typeorm -- migration:generate -n",
    "migration:revert": "npm run typeorm -- migration:revert",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "NODE_ENV=seed ts-node -r tsconfig-paths/register src/seeder.ts",
    "generate-session-key": "npx @fastify/secure-session > .secret-key"
  },
  "dependencies": {
    "@azure/core-auth": "^1.4.0",
    "@azure/event-hubs": "^5.8.0",
    "@fastify/swagger": "^6.1.1",
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.18",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/microservices": "^7.6.18",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/passport": "^7.1.6",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/platform-fastify": "^7.6.18",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/terminus": "^7.2.0",
    "@nestjs/typeorm": "^7.1.5",
    "@nominal-systems/dmi-engine-common": "^0.6.27",
    "argon2": "^0.28.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "faker": "^5.5.3",
    "fastify": "^3.14.0",
    "fastify-cookie": "^5.7.0",
    "fastify-passport": "^0.6.0",
    "fastify-session": "^5.2.1",
    "fastify-static": "^4.7.0",
    "fastify-swagger": "^5.2.0",
    "is-my-json-valid": "^2.20.5",
    "moment": "^2.30.1",
    "mongoose": "^5.13.4",
    "mongoose-sequence": "^6.0.1",
    "mqtt": "^4.2.8",
    "mysql2": "^2.2.5",
    "nanoid": "^3.1.23",
    "nanoid-dictionary": "^4.3.0",
    "nest-winston": "^1.6.2",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "passport-openidconnect": "^0.1.2",
    "jwks-rsa": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "typeorm": "^0.2.36",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.18",
    "@types/express": "^4.17.21",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.24",
    "@types/nanoid-dictionary": "^4.2.0",
    "@types/node": "^20.5.9",
    "@types/passport": "^1.0.17",
    "@types/passport-http": "^0.3.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-openidconnect": "^0.1.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.29.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "^2.3.2",
    "sqlite3": "^5.1.1",
    "supertest": "^6.1.4",
    "ts-jest": "^29.3.2",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "version": "^0.1.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
