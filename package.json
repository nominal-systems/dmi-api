    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.3.2",
    "supertest": "^6.1.4",
    "ts-jest": "^26.5.6",
    "ts-loader": "^8.3.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "version": "^0.1.2"
